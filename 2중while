package org.example;
//2중 while문 사용
public class prac6 {
    public static void main(String[] args) {

        int i = 1;
        while (i <= 10) {
//            System.out.println(1); // while문
//            System.out.println(2);
//            System.out.println(3);
            //System.out.println("i: "+ i);
            int k = 1;
            while (k <= 3) {
                System.out.println(k);
                k++;
            }
            i++;

        }
    }
}
