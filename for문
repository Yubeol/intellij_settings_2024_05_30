package org.example;
//2중 while문 사용
public class prac6 {
    public static void main(String[] args) {

       for (int i= 1; i <=3; i++){
           System.out.println("i : "+i);
           for (int k = 1; k <=3; k++){
               System.out.println(k);
           }
//            System.out.println(1); // while문
//            System.out.println(2);
//            System.out.println(3);
            //System.out.println("i: "+ i);

            }


        }
    }

